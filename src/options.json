{
    "browser": {
        "headless": false,
        "slowMo": 0,
        "devtools": false,
        "timeout": 1000000000,
        "excludeSwitches": "enable-automation",
        "args": [
            "--no-sandbox",
            "--disable-web-security",
            "--disable-setuid-sandbox",
            "--allow-file-access-from-files"
        ]
    },
    "storage": "./storage/",
    "files_path": "./storage/files/",
    "debugging": false,
    "proxyRotation": false,
    "saveCaptchan": false,
    "max_tries": 15,
    "pdf_missing_threshold": 0,
    "minutesToTimeout": 10,
    "documents": {
        "DocumentosGenerales": {
            "filters": {
                "documento": "",
                "fecha": "2022",
                "nombre": "",
                "cargo": ""
            }
        },
        "DocumentosJuridicos": {
            "filters": {
                "nombre de acto juuridico": "",
                "documento": "",
                "fecha": "2022"
            }
        },
        "DocumentosEconomicos": {
            "filters": {
                "documento": "",
                "fecha": "2022"
            }
        },
        "ValoresPagados": {
            "filters": {
                "rubro": "",
                "fecha de emision": "2022",
                "fecha de vencimiento": "",
                "referencia": "",
                "total": ""
            }
        },
        "AdministradoresActuales": {
            "filters": {
                "identificación": "",
                "nombre": "2022",
                "nacionalidad": "",
                "cargo": "",
                "fecha nombramiento": "",
                "periodo": "",
                "número registro mercantil": "",
                "fecha registro mercantil": "",
                "RL o ADM": ""
            }
        },
        "AdministradoresAnteriores": {
            "filters": {
                "identificación": "",
                "nombre": "2022",
                "nacionalidad": "",
                "cargo": "",
                "fecha nombramiento": "",
                "periodo": "",
                "número registro mercantil": "",
                "fecha registro mercantil": "",
                "RL o ADM": ""
            }
        },
        "Accionistas": {
            "filters": {
                "identificacion": "",
                "nombre": "",
                "nacionalidad": "",
                "tipo inversion": "",
                "capital": "",
                "restriccion": ""
            }
        }
    }
}
