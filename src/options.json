{
    "browser": {
        "headless": false,
        "slowMo": 0,
        "devtools": false ,
        "excludeSwitches": "enable-automation",
        "args": [
            "--no-sandbox",
            "--disable-web-security",
            "--disable-setuid-sandbox",
            "--allow-file-access-from-files"
        ]
    },
    "storage": "./storage/",
    "files_path": "./storage/files/",
    "debugging": false,
    "proxyRotation": false,
    "saveCaptchan": false,
    "max_tries": 15,
    "pdf_missing_threshold": 0,
    "minutesToTimeout": 10,
    "concurrent_processes": 20
}
