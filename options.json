{
    "browser": {
        "headless": true,
        "slowMo": 1,
        "devtools": false ,
        "excludeSwitches": "enable-automation",
        "args": [
            "--no-sandbox",
            "--disable-web-security",
            "--disable-setuid-sandbox",
            "--allow-file-access-from-files"
        ]
    },
    "debugging": true,
    "proxyRotation": true,
    "saveCaptchan": true,
    "triesWithProxies": 8,
    "triesPerProxy": 5,
    "minutesToTimeout": 5,
    "nameQueryTimeout": 3,
    "concurrent_processes": 1
}
